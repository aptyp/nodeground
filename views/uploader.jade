extends layout

block content
  .container
    h1 Upload your images here
    br
    form(method="post",enctype="multipart/form-data",action="/upload",  style='max-width: 300px;')
      .form-group
        input(type="file",name="upl")
      .form-group
        input(type="text",name="title",placeholder="Description of image")
      button.btn.btn-default(type='submit') Upload! 
      &nbsp;
      a(href='/')
        button.btn.btn-primary(type="button") Cancel
